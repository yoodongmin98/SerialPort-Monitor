prefix=C:/Program Files/CURL
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
supported_protocols="DICT FILE FTP GOPHER HTTP IMAP IPFS IPNS LDAP LDAPS MQTT POP3 RTSP SMB SMTP TELNET TFTP WS"
supported_features="alt-svc AsynchDNS IPv6 Largefile NTLM threadsafe UnixSockets"

Name: libcurl
URL: https://curl.se/
Description: Library to transfer files with HTTP, FTP, etc.
Version: 8.11.1
Requires: 
Requires.private: 
Libs: -L${libdir} -lcurl -lws2_32 -lbcrypt -lwldap32 -ladvapi32 -lcrypt32
Libs.private:  -lws2_32 -lbcrypt -lwldap32 -ladvapi32 -lcrypt32
Cflags: -I${includedir} -DCURL_STATICLIB
Cflags.private: -DCURL_STATICLIB
